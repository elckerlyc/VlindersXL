<!--

//aantal fotos =

var tal=44 + 1;
var lastday = 28;
var firstnight = 31;


var s1=0,s2=0;s3=0;s4=0;

var side=1;
var cur=0;
	function flip() {
	//document.getElementById('hier').innerHTML=side
	if (side==1) {plus="b"} else {plus=""}
		for (i=1;i<tal ;i++ )
		{
			if (i<lastday) {
				document.getElementById('img'+i).src="img/vlindersphgroot"+plus+i+".jpg"; }
			
		}
		document.getElementById('icon').src="img/dagpauw"+plus+".png"

	if (side==1) {side=0} else {side=1}
	}


	
// Script to open and close sidebar
function w3_open() {
    document.getElementById("mySidebar").style.display = "block";
    document.getElementById("myOverlay").style.display = "block";
	s4=1;
	
}
 
function w3_close() {
    document.getElementById("mySidebar").style.display = "none";
    document.getElementById("myOverlay").style.display = "none";
	s4=0;
}

function previous() {
	a=cur-1
	if (a==firstnight-1) {a=lastday-1}
	if (a>0) 	{show(a)}
}
function next() {
	a=cur+1
	if (a==lastday) {a=firstnight}
	if (a<(tal+1)) 	{show(a)}
}

function show(a) {
			cur=a;
			if (side==1) {plus=""} else {plus="b"}
			if (a>lastday) {document.getElementById('foto').src="img/vlinderstbgroot"+a+".jpg";
			document.getElementById('flip').style.visibility='hidden'}
			else {		document.getElementById('foto').src="img/vlindersphgroot"+plus+a+".jpg";
			document.getElementById('flip').style.visibility='visible'}
			document.getElementById('naam').innerHTML=vlinders[a].naam;
			document.getElementById('extranaam').innerHTML=vlinders[a].extranaam;
			document.getElementById('familie').innerHTML=vlinders[a].familie;
			document.getElementById('tekst').innerHTML=vlinders[a].tekst;

			document.getElementById('copy').innerHTML="";
			x=vlinders[a].copyright1;
			if (plus=="b") {x=vlinders[a].copyright2;}

			if (x) {document.getElementById('copy').innerHTML="foto &copy; "+x;}


			document.getElementById('extratekst').innerHTML=vlinders[a].extratekst;
			document.getElementById('next').setAttribute('style', 'color:black !important');
			document.getElementById('previous').setAttribute('style', 'color:black !important');
			if (a==tal) {
				document.getElementById('next').setAttribute('style', 'color:gray !important');
			}
			if (a==1) {
				document.getElementById('previous').setAttribute('style', 'color:gray !important');
			}

			document.getElementById('id01').style.display='block'
			document.getElementById('knop').focus();
			s1=1;
}

function flipme() {
			a=cur
			flip();
			if (side==1) {plus=""} else {plus="b"}
			document.getElementById('foto').src="img/vlindersphgroot"+plus+a+".jpg";
			
			document.getElementById('copy').innerHTML="";
			x=vlinders[a].copyright1;
			if (plus=="b") {x=vlinders[a].copyright2;}
			if (x) {document.getElementById('copy').innerHTML="foto &copy; "+x;}
			//document.getElementById('knop').focus();
}
function splashme() {




	x=document.getElementById('splash');
	// Code for Chrome, Safari and Opera
	//x.addEventListener("webkitAnimationStart", myStartFunction);
	//x.addEventListener("webkitAnimationEnd", myEndFunction);

// Standard syntax
	//x.addEventListener("animationstart", myStartFunction);
	//x.addEventListener("animationend", myEndFunction);
	x.style.display='block';
	s5=1;
	window.setTimeout(closeSplash, 5000);
	//

//document.getElementById('hier').innerHTML=document.getElementById('splash').getAttribute("display");


	//
}

function myEndFunction() {
document.getElementById('body').style.opacity=1;
}
function myStartFunction() {
document.getElementById('body').style.opacity=0.85;
}

function openme(a) {
	if (a==1) {document.getElementById('id01').style.display='block';s1=1;}
	if (a==2) {document.getElementById('id02').style.display='block';s2=1;}
	if (a==3) {document.getElementById('id03').style.display='block';s3=1;}
	if (a==5) {document.getElementById('splash').style.display='block';s5=1;}
	
}
function closeme(a) {
	if (a==1) {document.getElementById('id01').style.display='none';s1=0;}
	if (a==2) {document.getElementById('id02').style.display='none';s2=0;}
	if (a==3) {document.getElementById('id03').style.display='none';s3=0;}
	if (a==5) {document.getElementById('splash').style.display='none';s5=0;}
	
}
function closeSplash() {
	//alert('ja hoor');
	document.getElementById('splash').style.display='none';
	s5=0;
}
