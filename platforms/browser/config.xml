<?xml version='1.0' encoding='utf-8'?>
<widget id="nl.stippen.vlinders" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Vlinders Extra</name>
    <description>
        Vlinders in de Tuin met Extra Vlinders.
    </description>
    <author email="stichting.stippen@gmail.com" href="http://stippen.nl">
        stichting Stippen.nl
    </author>
    <content src="index.html" />
    <icon height="512" src="www/res/icon/512x.png" width="512" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="SplashScreenDelay" value="20000" />
</widget>
